<?xml version="1.0"?>
<config>
    <modules>
        <Autel_Catalog>
            <version>0.2.1</version>
        </Autel_Catalog>
    </modules>
      <depends>
        <Mage_Catalog />
      </depends>    
    <global>
        <blocks>
            <autelcatalog>
                <class>Autel_Catalog_Block</class>
            </autelcatalog>
        </blocks>
        <helpers>
            <autelcatalog>
                <class>Autel_Catalog_Helper</class>
            </autelcatalog>
        </helpers>        
        <models>    
            <autelcatalog> 
                <class>Autel_Catalog_Model</class>
                <resourceModel>autelcatalog_mysql4</resourceModel>
            </autelcatalog>
            <autelcatalog_mysql4>
                <class>Autel_Catalog_Model_Mysql4</class>
                <entities>
                    <import>
                        <table>autel_connector_import</table>
                    </import>
                    <message>
                        <table>autel_connector_import_message</table>
                    </message>
                </entities>
            </autelcatalog_mysql4>
            <catalog>
                <rewrite>
	             	<product_api>Autel_Catalog_Model_Product_Api</product_api>
                        <product_api_v2>Autel_Catalog_Model_Product_Api_V2</product_api_v2>
              	</rewrite>
                <rewrite>
	             	<product_attribute_api>Autel_Catalog_Model_Product_Attribute_Api</product_attribute_api>
                        <product_attribute_api_v2>Autel_Catalog_Model_Product_Attribute_Api_V2</product_attribute_api_v2>
              	</rewrite>
                <rewrite>
	             	<product_attribute_set_api>Autel_Catalog_Model_Product_Attribute_Set_Api</product_attribute_set_api>
                        <product_attribute_set_api_v2>Autel_Catalog_Model_Product_Attribute_Set_Api_V2</product_attribute_set_api_v2>
              	</rewrite>                
                <rewrite>
                        <product_attribute_media_api_v2>Autel_Catalog_Model_Product_Attribute_Media_Api_V2</product_attribute_media_api_v2>
              	</rewrite>                                
            </catalog>
        </models>
        <resources>
            <autelcatalog_setup>
                <setup>
                    <module>Autel_Catalog</module>
                    <!--<class>Mage_Core_Model_Resource_Setup</class>-->
                </setup>                
                <connection>
                    <use>core_setup</use>
                </connection>                
            </autelcatalog_setup>
            <autelcatalog_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </autelcatalog_write>
            <autelcatalog_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </autelcatalog_read>
        </resources>

    </global>
    <frontend> 
        <routers>
            <autelapi>
                <use>standard</use>
                    <args>
                        <module>Autel_Catalog</module>
                        <frontName>autelapi</frontName>
                    </args>
            </autelapi>
        </routers>    
    </frontend>
    
    <adminhtml>           
        <acl>
            <resources>
                <admin>
                    <children>
                        <system>
                            <children>
                                <config>
                                    <children>
                                        <autelconnector>
                                            <title>Autel Connector</title>
                                        </autelconnector>
                                    </children>
                                </config>
                            </children>
                        </system>
                    </children>
                </admin>
            </resources>
        </acl>
    </adminhtml>
    
</config>